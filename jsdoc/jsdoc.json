{
    "docdash": {
        "static": true,
        "sort": true
    },
    "opts": {
        "destination": "./doc",
        "encoding": "utf8",
        "recurse": true,
        "template": "./node_modules/docdash",
        "verbose": true
    },
    "plugins": [
        "plugins/markdown",
        "./jsdoc/plugins/chainable"
    ],
    "source": {
        "include": "./lib/",
        "includePattern": ".*/?.+\\.js$"
    },
    "tags": {
        "allowUnknownTags": false
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
